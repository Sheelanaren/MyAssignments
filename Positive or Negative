package week1.day2;

public class PositiveNegative {

	public static void main(String[] args) {
		int num=35;
		if (num>0)
		{
			System.out.println("Positive Number");
		}
		else if (num<0)
		{
			System.out.println("Negative Number");
		}
		else 
		{
			System.out.println("The number is neither positive nor negative");
		}
	}

}
